<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo2_test2b.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <TextField fx:id="ggrade_id" text="grade_id" />
  <TextField fx:id="gstudent_name" text="student_name" />
  <TextField fx:id="gsubject" text="subject" />
  <TextField fx:id="gscore" text="score" />
  <Button onAction="#InsertData" text="Insert" />
  <Button mnemonicParsing="false" onAction="#UpdateData" text="Update " />
  <Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
  <Button mnemonicParsing="false" onAction="#LoadData" text="Load Data" />
  <Button mnemonicParsing="false" onAction="#onHelloButtonClick" text="Fetch" />
  <TableView fx:id="productTable" prefHeight="200.0" prefWidth="200.0">
    <columns>
      <TableColumn fx:id="grade_id" prefWidth="75.0" text="grade_id" />
      <TableColumn fx:id="student_name" prefWidth="75.0" text="student_name" />
      <TableColumn fx:id="subject" prefWidth="75.0" text="subject" />
      <TableColumn fx:id="score" prefWidth="75.0" text="score" />
    </columns>
  </TableView>
  <Label text="Label" />
</VBox>
